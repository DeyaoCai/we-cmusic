
  .sheet-list{
    display: flex;
    background-color: #fff;
    padding: .65rem;
    border-top: 1px dashed #eee;
  }
  .sheet-list>view{
    text-align: left;
    flex: 1;
  }
  .sheet-list>image{
    width: 3rem;
    height: 3rem;
  }
  .sheet-cont{
    padding-bottom: .4rem;
    padding-left: .4rem;
  }
  .sheet-name{
    display: inline-block;
    font-size: .65rem;
  }
  .sheet-artists{
    padding-left: .4rem;
    display: inline-block;
    color: #999;
    font-size: .55rem;
  }
  .sheet-reason{
    padding-left: .4rem;
    color: #999;
    font-size: .55rem;
  }

